{
  "LABEL": {
    "name": "PR title must use conventional commits",
    "color": "EEEEEE"
  },
  "CHECKS": {
    "prefixes": ["fix: ", "feat: ", "build: ", "chore: ", "style: ", "refactor: ", "perf: ", "test: "],
    "regexp": "docs\\(v[0-9]\\): ",
    "regexpFlags": "i",
    "ignoreLabels" : ["dont-check-PRs-with-this-label", "meta"]
  },
  "MESSAGES": {
    "success": "Success",
    "failure": "Failing CI test",
    "notice": ""
  }
}




