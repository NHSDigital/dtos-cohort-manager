{
    "version": "2.0",
    "logging": {
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "excludedTypes": "Request"
            },
            "enableLiveMetricsFilters": true
        },
        "concurrency": {
            "dynamicConcurrencyEnabled": true,
            "snapshotPersistenceEnabled": true
        },
        "queues": {
            "maxPollingInterval": 1000,
            "batchSize": 32,
            "maxDequeueCount": 1,
            "newBatchThreshold": 8
          }
    }
}
