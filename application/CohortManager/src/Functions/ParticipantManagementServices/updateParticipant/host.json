{
    "version": "2.0",
    "functionTimeout": "01:00:00",
    "concurrency": {
      "dynamicConcurrencyEnabled": true,
      "snapshotPersistenceEnabled": true
    },
    "extensions": {
      "queues": {
        "batchSize": 8,
        "newBatchThreshold": 4,
        "visibilityTimeout": "00:01:00"
      }
    },
    "logging": {
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "excludedTypes": "Request"
            },
            "enableLiveMetricsFilters": true
        }
    }
}
