[
  {
    "WorkflowName": "TransformData",
    "Rules": [
      {
        "RuleName": "1.NamePrefix.ExceedsMaximumLength",
        "Expression": "participant.NamePrefix.Length <= 35",
        "Actions": {
          "OnFailure": {
            "Name": "OutputExpression",
            "Context": {
              "Expression": "participant.NamePrefix.Substring(0, 35)"
            }
          }
        }
      },
      {
        "RuleName": "2.Gender.Not-0-1-2-or-9",
        "Expression": "participant.Gender == 0 || participant.Gender == 1 || participant.Gender == 2 || participant.Gender == 9",
        "Actions": {
          "OnFailure": {
            "Name": "OutputExpression",
            "Context": {
              "Expression": "9"
            }
          }
        }
      },
      {
        "RuleName": "3.OtherGivenNames.ExceedsMaximumLength",
        "Expression": "participant.OtherGivenNames.Length <= 100",
        "Actions": {
          "OnFailure": {
            "Name": "OutputExpression",
            "Context": {
              "Expression": "participant.FirstName.Substring(0, 100)"
            }
          }
        }
      }
    ]
  }
]
