[
  {
    "WorkflowName": "TransformData",
    "Rules": [
      {
        "RuleName": "1.NamePrefix.ExceedsMaximumLength",
        "Expression": "participant.NamePrefix.Length <= 35",
        "Actions": {
          "OnFailure": {
            "Name": "OutputExpression",
            "Context": {
              "Expression": "participant.NamePrefix.Substring(0, 35)"
            }
          }
        }
      }
    ]
  },
  {
    "RuleName": "2.GivenName.ExceedsMaximumLength",
    "Expression": "participant.GivenName.Length <= 35",
    "Actions": {
      "OnFailure": {
        "Name": "OutputExpression",
        "Context": {
          "Expression": "participant.GivenName.Substring(0, 35)"
        }
      }
    }
  },
  {
    "RuleName": "3.OtherGivenNames.ExceedsMaximumLength",
    "Expression": "participant.OtherGivenNames.Length <= 100",
    "Actions": {
      "OnFailure": {
        "Name": "OutputExpression",
        "Context": {
          "Expression": "participant.OtherGivenNames.Substring(0, 100)"
        }
      }
    }
  },
  {
    "RuleName": "4.FamilyName.ExceedsMaximumLength",
    "Expression": "participant.FamilyName.Length <= 35",
    "Actions": {
      "OnFailure": {
        "Name": "OutputExpression",
        "Context": {
          "Expression": "participant.FamilyName.Substring(0, 35)"
        }
      }
    }
  }
]
