{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingExcludedTypes": "Request",
      "samplingSettings": {
        "isEnabled": true
      }
    },
    "logLevel": {
      "Default": "Warning",
      "Host.Results": "Error",
      "Host.Http": "Warning",
      "Function": "Information"
    }
  }, 
  "extensions": {
    "durableTask": {
      "storageProvider": {
        "type": "AzureStorage",
        "maxDurableActivityFunctionTimeout": "30:00:00", 
        "maxOrchestrationFunctionTimeout": "24:00:00",
        "extendedSessionsEnabled": true,
        "historyRetentionInDays": 7
      }
    }
  }
}