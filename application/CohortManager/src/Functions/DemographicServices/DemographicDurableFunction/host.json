{
  "version": "2.0",
  "functionTimeout": "01:00:00", 
  "logging": {
    "applicationInsights": {
      "samplingExcludedTypes": "Request",
      "samplingSettings": {
        "isEnabled": true
      }
    }
  }, 
  "extensions": {
    "durableTask": {
      "hubName": "MyTaskHub",
      "storageProvider": {
        "type": "AzureStorage",
        "maxDurableActivityFunctionTimeout": "30:00:00", 
        "maxOrchestrationFunctionTimeout": "24:00:00",
        "extendedSessionsEnabled": true,
        "historyRetentionInDays": 90
      }
    }
  }
}